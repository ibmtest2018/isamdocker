	<security-constraint>
		<display-name>RoleA</display-name>
		<web-resource-collection>
			<web-resource-name>
				RoleAResources
			</web-resource-name>
			<url-pattern>/rolea/*</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>RoleA</role-name>
		</auth-constraint>
	</security-constraint>

	<security-constraint>
		<display-name>RoleB</display-name>
		<web-resource-collection>
			<web-resource-name>
				RoleBResources
			</web-resource-name>
			<url-pattern>/roleb/*</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
		<auth-constraint>
			<role-name>RoleB</role-name>
		</auth-constraint>
	</security-constraint>
</web-app>
